
(begin test

  (func args_0 () (
    ("success!")
  ))

  (func args_1 (_) (
    ("success!")
  ))

  (func args_2 (_ _) (
    ("success!")
  ))

  (func test_func (msg1 f1 msg2 f2) (
    (put msg1)
    (putln f1)
    (put msg2)
    (putln f2)
  ))

  (func test_all () (
    (test_func "Testing 0 arg function call with 0 args (should pass)... "
               args_0
               "Testing 0 arg function call with 1 args (should fail)... "
               (args_0 1))
    (test_func "Testing 1 arg function call with 1 args (should pass)... "
               (args_1 1)
               "Testing 1 arg function call with 0 args (should fail)... "
               args_1)
    (test_func "Testing 2 arg function call with 2 args (should pass)... "
               (args_2 1 1)
               "Testing 2 arg function call with 1 args (should fail)... "
               (args_2 1))
  ))

  (putln "\nInitiating function argument testing...\n")
  (test_all)

)