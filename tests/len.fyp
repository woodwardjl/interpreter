
(begin test

  (func str_len_with_count (str count) (
    (if (== (len str) count)
        (putln "success!")
        (putln "failure"))
  ))

  (func str_len_general () (
    (len "12345")
    (putln "success!")
  ))

  (func int_len () (
    (var x (len 10))
    (putln x)
  ))

  (func test_func (msg f) (
    (put msg)
    (f)
  ))

  (func test_all () (
    (test_func "Testing len of integer (should fail)... " int_len)
    (test_func "Testing len of string (should pass)... " str_len_general)
    (test_func "Testing that len if '12345' is 5 (should pass)... "
               (str_len_with_count "12345" 5))
  ))

  (putln "\nInitiating len testing...\n")
  (test_all)

)