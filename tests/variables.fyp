
(begin test

  (func return_var () (
    ("Hello, World!")
  ))

  (func test_func (msg f) (
    (put msg)
    (f)
    (putln "success!")
  ))

  -- Local variable
  (func test_all () (
    (var x "12345")
    (test_func "Testing local variable declaration (should pass)... " x)
  ))

  -- Should now fail test, as it's out of scope!
  (putln "\nInitiating variable testing...\n")
  (test_all)
  (put "Testing out of scope var (should fail)... ")
  (putln x)

)